{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockbite/dynamic-design",
  "version": "0.1.0",
  "title": "Dynamic Design",
  "category": "blockbite",
  "icon": "flag",
  "description": "A block to present dynamic content",
  "usesContext": [
    "blockbite/dynamicDesignActive"
  ],
  "attributes": {
    "designId": {
      "type": "string",
      "default": ""
    },
    "contentId": {
      "type": "string",
      "default": ""
    },
    "enabled": {
      "type": "boolean",
      "default": false
    },
    "metadata": {
      "type": "object",
      "default": {
        "name": "Dynamic Design"
      }
    }
  },
  "supports": {
    "html": false,
    "anchor": true,
    "color": {
      "gradients": true,
      "link": true,
      "background": true,
      "text": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "renaming": true
  },
  "textdomain": "blockbite",
  "editorScript": "file:./index.js"
}