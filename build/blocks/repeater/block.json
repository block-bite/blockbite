{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockbite/repeater",
  "version": "0.1.0",
  "title": "Repeater",
  "category": "blockbite",
  "icon": "flag",
  "description": "repeater block",
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "repeaterItems": {
      "type": "array",
      "default": []
    },
    "repeaterType": {
      "type": "string",
      "default": "none"
    },
    "flexStyle": {
      "type": "array",
      "default": []
    },
    "flexClass": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "background": true,
      "text": true
    },
    "dimensions": {
      "minHeight": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true
    },
    "align": true
  },
  "textdomain": "blockbite",
  "editorScript": "file:./index.js"
}